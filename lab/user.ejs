<% var response = {
    "name": "John",
    "email": "john@example.com"
}; %>
[
    {
        "predicates": [
            {
                "and": [
                    {
                        "equals": {
                            "path": "/users/123",
                            "method": "GET"
                        }
                    }
                ]
            }
        ],
        "responses": [
            {
                "is": {
                    "statusCode": 200,
                    "headers": {
                        "Content-Type": "application/json"
                    },
                    "body": <%= JSON.stringify(response) %>
                }
            }
        ]
    }
]